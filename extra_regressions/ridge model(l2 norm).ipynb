   "source": [
    "from sklearn.linear_model import Ridge\n",
    "import numpy as np\n",
    "n_samples, n_features = 10,5\n",
    "rng = np.random.RandomState(0)\n",
    "y = rng.randn(n_samples)\n",
    "x = rng.randn(n_samples , n_features)\n",
    "clf = Ridge(alpha =1.0)\n",
    "clf.fit(x,y)"
   ]
